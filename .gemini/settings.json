{
	"general": {
		"previewFeatures": true
	},
	"mcpServers": {
		"@magicuidesign/mcp": {
			"command": "npx",
			"args": ["-y", "@magicuidesign/mcp@latest"]
		},
		"context7": {
			"command": "npx",
			"args": ["-y", "@upstash/context7-mcp", "--api-key", "$CONTEXT7_API_KEY"],
			"env": {
				"CONTEXT7_API_KEY": "$CONTEXT7_API_KEY"
			}
		}
	}
}
